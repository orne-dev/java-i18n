<bindings version="2.1"
        xmlns="http://java.sun.com/xml/ns/jaxb"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
        xmlns:annox="http://annox.dev.java.net"
        extensionBindingPrefixes="xjc annox"
        xsi:schemaLocation="
            http://java.sun.com/xml/ns/jaxb https://java.sun.com/xml/ns/jaxb/bindingschema_2_1.xsd
        ">
  <bindings scd="x-schema::tns" xmlns:tns="http://orne.dev/i18n/">
    <globalBindings>
      <serializable uid="1" />
      <xjc:simple />
    </globalBindings>
    <schemaBindings>
      <!--
      <package name="dev.orne.i18n"/>
      -->
      <package name="dev.orne.i18n.jaxb"/>
    </schemaBindings>
    <bindings scd="~tns:string">
      <class name="XmlI18nString" />
    </bindings>
    <bindings scd="~tns:translation">
      <!--
      <class name="XmlI18nStringTranslation" />
      -->
    </bindings>
  </bindings>
</bindings>
